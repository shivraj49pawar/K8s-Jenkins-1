pipeline {
  agent any
  stages {
    stage('Hello') {
      steps {
        writeFile(file: 'Hello.txt', text: 'Hi WP')
      }
    }

    stage('Read YAML') {
      steps {
        readFile 'pods.yml'
      }
    }

    stage('Run Yaml') {
      steps {
        withKubeConfig(caCertificate: ' ', clusterName: 'kubernetes', serverUrl: ' https://192.168.1.41:6443', namespace: 'jenkiswp', credentialsId: 'b144d276-234c-4d1c-a98f-2d58f206ddf9')
      }
    }

  }
}